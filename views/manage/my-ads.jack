<h1 class="page-margin-top"Manage Ads</h1>

<div class="my-grid-container">
    {{#each list}}
    <div class="product">
        <a href="/my-ads-detail/{{this._id}}">
            <p>{{this.title}}</p>
            <a href="/edit-ads/{{this._id}}">Edit</a> <br>
            <a href="/my-ads/delete/{{this._id}}" onclick="return confirm('Are you sure you want to delete this Ads? ');">Delete</a>
        </a>
    </div>
    {{/each}}
</div>